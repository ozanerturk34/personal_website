{
  "name": "grpc",
  "version": "1.0.0",
  "description": "gRPC server for communication between strapi and next",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npm run build:proto",
    "build": "npm run build:typescript",
    "build:typescript": "tsc -p .",
    "build:proto": "./compile-proto.sh",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check \"**/*.{ts,js,json,svg,md,yml}\"",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint:fix:prettier && npm run lint:eslint -- --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.6",
    "protoc-gen-ts": "^0.8.6",
    "typescript": "5.0.4"
  }
}
